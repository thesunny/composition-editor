    "development": {
      "presets": [
        [
          "next/babel",
          {
            "preset-env": {},
            "transform-runtime": {},
            "styled-jsx": {},
            "class-properties": {}
          }
        ]
      ],
      "plugins": ["module:babel-root-slash-import"]
    },
    "production": {
      "presets": [
        [
          "next/babel",
          {
            "preset-env": {},
            "transform-runtime": {},
            "styled-jsx": {},
            "class-properties": {}
          }
        ]
      ],
      "plugins": ["module:babel-root-slash-import"]
    },
    "test": {
      "presets": [
        "react",
        ["env", { "targets": { "node": "8.11.1" } }],
        "stage-0"
      ],
      "plugins": ["babel-root-slash-import"]
    }
  }
